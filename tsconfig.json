{
  "compilerOptions": {
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "module": "CommonJS",
      "target": "ES2015",
      "sourceMap": true,
      "esModuleInterop": true,
      "rootDir": "./src",
      "outDir": "./dist"
  },
  "include": [
      "src/**/*"
  ],
  "exclude": [
      "node_modules",
      "dist"
  ]
}
